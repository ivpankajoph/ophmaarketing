# Application
NODE_ENV=production
SESSION_SECRET=your_session_secret_here

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o

# Database (PostgreSQL)
DATABASE_URL=postgresql://USER:PASSWORD@HOST:PORT/DATABASE?sslmode=require
PGHOST=your_pg_host
PGPORT=5432
PGUSER=your_pg_user
PGPASSWORD=your_pg_password
PGDATABASE=your_pg_database

# MongoDB
MONGODB_URL=mongodb+srv://USERNAME:PASSWORD@cluster-url.mongodb.net/?retryWrites=true&w=majority

# WhatsApp / Meta API
WHATSAPP_TOKEN=your_whatsapp_token
WHATSAPP_TOKEN_NEW=your_new_whatsapp_token
WHATSAPP_PERMANENT_TOKEN=your_permanent_whatsapp_token
VERIFY_TOKEN=your_webhook_verify_token
PHONE_NUMBER_ID=your_phone_number_id
WABA_ID=your_waba_id
FB_PAGE_ID=your_facebook_page_id
FB_ACCESS_TOKEN=your_fb_access_token
FB_PAGE_ACCESS_TOKEN=your_fb_page_access_token

# SMTP (Email)
SMTP_EMAIL=your_smtp_email
SMTP_PASSWORD=your_smtp_password

# Google API
GOOGLE_API_KEY=your_google_api_key

